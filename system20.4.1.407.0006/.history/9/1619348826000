CREATE TABLE hastaneler(

 hastane_id VARCHAR2(5),
 hastane_ad VARCHAR2(25),
 hastane_sehir VARCHAR2(10),
 hastane_statu VARCHAR2 (3),
 
 CONSTRAINT pk_id PRIMARY KEY (hastane_id)
 
);

